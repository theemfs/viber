{
    "name": "nzx/viber",
    "description": "Viber notifications driver",
    "keywords": ["laravel", "notifications", "viber"],
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/notifications": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "NZX\\NotificationChannels\\Viber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NZX\\NotificationChannels\\Viber\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
